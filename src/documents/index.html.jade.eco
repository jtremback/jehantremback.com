---
title: "Welcome!"
layout: "default"
isPage: true
---

.container
  .background
  .surface

  <% for category in @site.categories: %>
  .wrapper
    .polaroid.<%- category.id %>(data-default-page='<%- category.default %>')
      img(src="images/<%- category.id %>.jpg")
      .title_bar
        .title <%- category.name %>
        a.link(href='#<%- category.default %>') Expander
  <% end %>


#popup
  .close.black
  #holder
