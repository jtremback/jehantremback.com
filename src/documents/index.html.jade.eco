---
layout: "default"
isPage: true
---

.container

  <% for category in @site.categories: %>
  .wrapper
    .polaroid.<%- category.id %>
      .img(style='background: url(images/<%- category.id %>.png);')
      .title_bar
        .title <%- category.name %>
        a.link(href='#<%- category.default %>') Expand
  <% end %>


#popup
  .close.black
  #content-wrapper
    #content-holder.grid
