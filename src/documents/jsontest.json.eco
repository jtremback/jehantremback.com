<% a = 0 %>
{<% for category in @site.categories: %>
	<% a++ %>
	"<%- category.id %>":
	<% catJSON = @getPagesByCategory(category.id ).toJSON() %>
	<% b = 0 %>
	[<% for page in catJSON:  %>
		<% b++ %>
		"<%- page.basename + if b is catJSON.length then '"' else '",' %> 
	<% end %>]
	<%- if a is @site.categories.length then '' else ',' %>
<% end %>}


