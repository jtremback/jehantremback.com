<ul class="tabnav">
  <% for page in this.getPagesByCategory("#{@document.category}").toJSON():  %>
  <li class="<%= if page.id is @document.id then 'active' else 'inactive' %>">
    <a href="#<%= page.basename %>">
      <%= page.title %>
    </a>
  </li>
  <% end %>
</ul>

<div id="content"><%- @content %></div>
